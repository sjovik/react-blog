@import '../../frame/theme'

.container
  width 100%
  height 100%
  position relative
  background color-sub--light
  border-radius 50%
  padding 10%
  box-sizing border-box

.bar
  width 80%
  height 2px
  position absolute
  top 50%
  transform translateY(-50%)
  background #fff
  
  &:nth-child(1)
    transform rotate(90deg)
  
